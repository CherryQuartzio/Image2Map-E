{
  "schemaVersion": 1,
  "id": "image2map",
  "version": "${version}",

  "name": "Image2Map E",
  "description": "Turn images into maps! This version of the mod has been customized for more suitable usage in survival",
  "authors": [
    "Essem",
    "Patbox",
    "CherryQuartzio"
  ],
  "contact": {
    "sources": "https://github.com/CherryQuartzio/Image2Map-E"
  },

  "license": "MIT",
  "icon": "assets/image2map/icon.png",

  "environment": "*",
